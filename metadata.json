    { "name": "puppet/epel", "version_requirement": ">= 2.0.0 < 4.0.0" },
    { "name": "puppetlabs/mysql", "version_requirement": ">= 2.0.0 < 11.0.0" },
    { "name": "tohuwabohu/patch", "version_requirement": ">= 1.0.0 < 2.0.0" },
    { "name": "puppetlabs/postgresql", "version_requirement": ">= 3.3.0 < 7.0.0" },
    { "name": "puppetlabs/stdlib", "version_requirement": ">=3.2.0 < 7.0.0" },
