{
  "name": "johanek-redmine",
  "version": "3.0.0",
  "author": "Johan van den Dorpe",
  "license": "Apache-2.0",
  "summary": "Module to install redmine",
  "source": "https://github.com/johanek/johanek-redmine",
  "project_page": "https://github.com/johanek/johanek-redmine",
  "issues_url": "https://github.com/johanek/johanek-redmine/issues",
